<% if session[:user_email] %>
    <% if session[:alert] %>
      <p><%= session[:alert] %></p>
      <div class="login-button">
        <%= link_to 'Login with Google', '/login', class: 'login-with-google' %>
      </div>
      <div class="login-button">
        <%= link_to 'Login as Admin', '/admin', class: 'login-with-google' %>
      </div>
    <% else %>
    <%= controller.redirect_to '/home' %>
  <% end %>
<% else %>
 <%= render 'login'%>
<% end %>

