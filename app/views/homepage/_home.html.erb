<div class="home-page">
  <div class="div">
    <div class="overlap-group">
        <a href="/home"><%= image_tag('home.png', class: 'icon-home', alt: 'Description') %></a>
        <%= image_tag('dashboard.png', class: 'dashboard-layout', alt: 'Description') %>
        <%= image_tag('brief.png', class: 'brief', alt: 'Description') %>
        <%= image_tag('alarm.png', class: 'alarm', alt: 'Description') %>
        <a href='/logout'><%= image_tag('logoutlogo.png', class: 'logout', alt: 'Description') %></a>
    </div>
    <div class='title'>
        <%= image_tag('tamu_circle_logo.png', class: 'tamu-circle-logo', alt: 'Description') %>
        <% if logged_in? %>
        <p class="welcome-to-phd">
          Welcome <%= current_user.user_name %> !
          <br><br> PhD. Application Review System
        </p>
        <% end %>
    </div>
    <div class="div-wrapper">
      <div class="text-wrapper">Search</div>
    </div>
    <img class="rectangle" src="img/rectangle-7.svg" />
    <div class="list-item">
      <div class="ellipse"></div>
      <div class="rectangle-2"></div>
      <div class="rectangle-3"></div>
      <div class="rectangle-4"></div>
    </div>
    <div class="list-item-2">
      <div class="rectangle-5"></div>
      <div class="rectangle-6"></div>
      <div class="rectangle-7"></div>
      <div class="ellipse-2"></div>
    </div>
    <div class="list-item-3">
      <div class="ellipse-3"></div>
      <div class="rectangle-5"></div>
      <div class="rectangle-6"></div>
      <div class="rectangle-7"></div>
    </div>
  </div>
</div>

